{
  "name": "htmx-lsp",
  "description": "Language Server for HTMX",
  "version": "0.0.1",
  "contributors": [
    "ThePrimeagen",
    "MNThomson"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ThePrimeagen/htmx-lsp"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "activationEvents": [
    "onLanguage:html"
  ],
  "main": "./extension",
  "contributes": {
    "configuration": {
      "title": "htmx-lsp",
      "properties": {
        "htmx-lsp.intreperterPath": {
          "type": "string",
          "default": "htmx-lsp",
          "description": "Path to the htmx-lsp binary"
        }
      }
    }
  },
  "dependencies": {
    "vscode-languageclient": "^8.1.0"
  }
}
